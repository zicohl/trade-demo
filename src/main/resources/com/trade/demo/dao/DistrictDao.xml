<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.trade.demo.dao.DistrictDao">
    <resultMap id="district" type="com.trade.demo.po.DistrictPo">
        <id property="id" column="id" />
        <result property="parentId" column="parent_id" />
        <result property="code" column="code" />
        <result property="name" column="name" />
        <result property="nameZh" column="name_zh" />
        <result property="orderNmuber" column="order_number" />
    </resultMap>

    <select id="findAllDistrict" resultMap="district">
        select id,parent_id,code,pinyin name, name||extra||suffix name_zh,order_number from district
    </select>
</mapper>
